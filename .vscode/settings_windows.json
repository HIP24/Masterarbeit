{
    "search.exclude": {
        "**/masterthesis-documentation/docs/resources/exe/*": true,
        "**/masterthesis-documentation/docs/resources/images/*": true,
        "**/masterthesis-documentation/docs/resources/scripts/*": true,
        "**/masterthesis-documentation/docs/sigmatek/trace-cmd/working_trace-cmd/*": true,
        "**/*.dat": true,
        "**/*.png": true,
        "**/*.jpg": true,
        "**/bkp.*": true
    },
    "files.exclude": {
        "**/Masterarbeit_Robotik_Pamuk.aux": true,
        "**/Masterarbeit_Robotik_Pamuk.bbl": true,
        "**/Masterarbeit_Robotik_Pamuk.blg": true,
        "**/Masterarbeit_Robotik_Pamuk.lof": true,
        "**/Masterarbeit_Robotik_Pamuk.log": true,
        "**/Masterarbeit_Robotik_Pamuk.lol": true,
        "**/Masterarbeit_Robotik_Pamuk.lot": true,
        "**/Masterarbeit_Robotik_Pamuk.run.xml": true,
        "**/Masterarbeit_Robotik_Pamuk.synctex.gz": true,
        "**/Masterarbeit_Robotik_Pamuk.toc": true,
        "**/Masterarbeit_Robotik_Pamuk.bcf": true,
        "**/Masterarbeit_Robotik_Pamuk.toc": true,
        "**/Masterarbeit_Robotik_Pamuk.toc": true,
        "**/else": true,
        "**/trash": true,
        "**/clean": true,
        "**/site": true,
        "**/twbook.pdf": true,
    },
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex and Copy",
            "tools": ["pdflatex", "copy_pdf"]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-laine-error",
                "%DOC%"
            ]
        },
        {
            "name": "copy_pdf",
            "command": "cmd",
            "args": [
                "/C",
                "copy",
                "/Y",
                "%DOCFILE%.pdf",
                "masterthesis-documentation\\docs"
            ],
            "env": {}
        }
    ],
    "editor.wordWrap": "on",

}