{
    "search.exclude": {
        "**/masterthesis-documentation/docs/resources/exe/*": true,
        "**/masterthesis-documentation/docs/resources/images/*": true,
        "**/masterthesis-documentation/docs/resources/scripts/*": true,
        "**/masterthesis-documentation/docs/sigmatek/trace-cmd/analysis/test/trace-Salamander4.dat": true,
        "**/masterthesis-documentation/docs/sigmatek/trace-cmd/analysis/test/trace.dat": true,
        "**/masterthesis-documentation/docs/sigmatek/trace-cmd/LTS/*": true,
        "**/masterthesis-documentation/docs/sigmatek/trace-cmd/working_trace-cmd/*": true
    },
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex and Copy",
            "tools": ["pdflatex", "copy_pdf"]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-laine-error",
                "%DOC%"
            ]
        },
        {
            "name": "copy_pdf",
            "command": "cmd",
            "args": [
                "/C",
                "copy",
                "/Y",
                "%DOCFILE%.pdf",
                "masterthesis-documentation\\docs"
            ],
            "env": {}
        }
    ]
    

}