FUNCTION VIRTUAL BusInterfaceVARAN::AddSDORequest
	VAR_INPUT
		pCmdSvr 	: ^SvrChCmd;
		Command 	: HINT;
		Offset 	: UDINT;
		Length 	: UDINT;
		pWriteBuf 	: ^void;
	END_VAR
	VAR_OUTPUT
		sd_retval 	: DINT;
	END_VAR

  // protect via mutex because this can be called from different task priorities
  OS_MT_WAIT(pv_MutexSDOs);

  // if the buffer is full, or the next element in the buffer is already in use
  if (SDOHandling.pWrite^.Command <> 0) then
    sd_retval := -2;
    OS_MT_SIGNAL(pv_MutexSDOs);
    return;
  end_if;
  
  if pCmdSvr = NIL then
    sd_retval := -3;
    return;
  end_if;
  
  if Length = 0 then
    sd_retval := -4;
  end_if;
  
  if Command =_BUS_IF_WRITE_VIA_SDO | Command = _BUS_IF_WRITE_VIA_SDO_CTRL then
    if pWriteBuf = NIL then
      sd_retval := -5;
      return;
    end_if;
  end_if;

  
  SDOHandling.pWrite^.pCallBackSvr  := pCmdSvr;
  SDOHandling.pWrite^.udLength      := Length;
  SDOHandling.pWrite^.udOffset      := Offset;
  SDOHandling.pWrite^.pWriteData    := pWriteBuf;
  // command is the last thing to store, because it indicates a complete entry
  SDOHandling.pWrite^.Command := Command;

  // we increment our used entries in the buffer (only for debugging)
  SDOHandling.EntriesUsed     += 1;

  // after we successfully added this SDO, set the write pointer to the next element
  if SDOHandling.pWrite = SDOHandling.pLastEntry then
    SDOHandling.pWrite := SDOHandling.pRingBufMemStart;
  else
    SDOHandling.pWrite += sizeof(t_SDORingBufEntry);
  end_if;

  sd_retval := 0;
  
  OS_MT_SIGNAL(pv_MutexSDOs);
  
END_FUNCTION


FUNCTION VIRTUAL BusInterfaceVARAN::CyclicSDOHandling
  VAR
		Length 	      : UDINT;
    pRingBufEntry : ^t_SDORingBufEntry;
    pCmdSvr       : ^SvrChCmd;
  	tempCmd       : CmdStruct;
    _result       : results;
    Offset : UDINT;
  END_VAR
  

  // protect via mutex because this can be called from different task priorities
  OS_MT_WAIT(pv_MutexSDOs);

  //If there is some work todo
  if SDOHandling.EntriesUsed then
  
    if SDOHandling.pActEntry & SDOHandling.pActEntry^.Command <> 0 then
    
      pRingBufEntry := SDOHandling.pActEntry;
    
      //State = 0 ... Start SDO access, 1 ... Get SDO access data and stop SDO
      if SDOHandling.pActEntry^.udState = 0 then
      
        Length := SDOHandling.pActEntry^.udLength;
        Offset := SDOHandling.pActEntry^.udOffset;
        
        case SDOHandling.pActEntry^.Command of
        
          _BUS_IF_READ_VIA_SDO:
          
            if ChangeAsyRd(Offset:=Offset$UINT, 
                           Length:=Length$USINT, 
                           RunState:=_BUS_IF_RUN_STATE_CONT_RUN,
                           bIsCtrlACC := FALSE) = FALSE then
              LogError("@009B (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the MEM ReadDO");
            end_if;
            
            SDOHandling.pActEntry^.udState := 1;      
          
          _BUS_IF_READ_VIA_SDO_CTRL:
          
            if ChangeAsyRd(Offset:=Offset$UINT, 
                           Length:=Length$USINT, 
                           RunState:=_BUS_IF_RUN_STATE_CONT_RUN,
                           bIsCtrlACC := TRUE) = FALSE then
              LogError("@009B (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the CTRL ReadDO");
            end_if;
            
            SDOHandling.pActEntry^.udState := 1;      
            
          _BUS_IF_WRITE_VIA_SDO:
          
            _memcpy(ptr1:=AsyMemWriteDO.pData, ptr2:=SDOHandling.pActEntry^.pWriteData, cntr:=SDOHandling.pActEntry^.udLength);
            
            if ChangeAsyWr(Offset:=Offset$UINT, 
                           Length:=Length$USINT, 
                           RunState:=_BUS_IF_RUN_STATE_CONT_RUN,
                           bIsCtrlACC := FALSE) = FALSE then
              LogError("@009B (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the MEM WriteDO");
            end_if;
            
            SDOHandling.pActEntry^.udState := 1;
          
          _BUS_IF_WRITE_VIA_SDO_CTRL:
                      
            _memcpy(ptr1:=AsyCtrlWriteDO.pData, ptr2:=SDOHandling.pActEntry^.pWriteData, cntr:=SDOHandling.pActEntry^.udLength);
            
            if ChangeAsyWr(Offset:=Offset$UINT, 
                           Length:=Length$USINT, 
                           RunState:=_BUS_IF_RUN_STATE_CONT_RUN,
                           bIsCtrlACC := TRUE) = FALSE then
              LogError("@009B (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the CTRL WriteDO");
            end_if;
            
            SDOHandling.pActEntry^.udState := 1;

          
        end_case;
      
      elsif SDOHandling.pActEntry^.udState = 1 then
      
        SDOHandling.EntriesUsed -= 1;
      
        // Call the corresponding function in the operator class
        tempCmd.uiCmd := _BUS_IF_SDO_RESPONSE;
        tempCmd.aPara[0] := SDOHandling.pActEntry^.Command$DINT;
        tempCmd.aPara[1] := SDOHandling.pActEntry^.udOffset$DINT;
        tempCmd.aPara[2] := SDOHandling.pActEntry^.udLength$DINT;
        
        tempCmd.aPara[3] := 0;
        
        case SDOHandling.pActEntry^.Command of
        
          _BUS_IF_READ_VIA_SDO:
          
            tempCmd.aPara[4] := AsyMemReadDO.pData$DINT;
            tempCmd.aPara[5] := AsyMemReadDO.Length;

          _BUS_IF_READ_VIA_SDO_CTRL:
          
            tempCmd.aPara[4] := AsyCtrlReadDO.pData$DINT;
            tempCmd.aPara[5] := AsyCtrlReadDO.Length;
            
          _BUS_IF_WRITE_VIA_SDO:
          
            tempCmd.aPara[4] := NIL$DINT;
            tempCmd.aPara[5] := 0;
          
          _BUS_IF_WRITE_VIA_SDO_CTRL:
          
            tempCmd.aPara[4] := NIL$DINT;
            tempCmd.aPara[5] := 0;
                    
        end_case;
        
        pCmdSvr := pRingBufEntry^.pCallBackSvr;
        
        if pCmdSvr$pVirtualBase^.NewInst(#tempCmd, #_result) <> READY then               
          LogError("@0099 (BusInterfaceVARAN::CyclicSDOHandling) SDO Error: Error of module at place 0x{0} while receiving the SDO reponse (which was an ErrorCode)");                              
        end_if;

        // the entry in the ringbuffer is free
        pRingBufEntry^.Command := 0;
        
        //Stop the DOs
        if ChangeAsyRd(Offset:=0, 
                       Length:=0, 
                       RunState:=_BUS_IF_RUN_STATE_NO_RUN,
                       bIsCtrlACC := TRUE) = FALSE then
          LogError("@0769 (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the CTRL ReadDO");
        end_if;
        
        if ChangeAsyRd(Offset:=0, 
                       Length:=0, 
                       RunState:=_BUS_IF_RUN_STATE_NO_RUN,
                       bIsCtrlACC := FALSE) = FALSE then
          LogError("@076A (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the MEM ReadDO");
        end_if;
        
        if ChangeAsyWr(Offset:=0, 
                       Length:=0, 
                       RunState:=_BUS_IF_RUN_STATE_NO_RUN,
                       bIsCtrlACC := TRUE) = FALSE then
          LogError("@076B (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the CTRL WriteDO");
        end_if;
        
        if ChangeAsyWr(Offset:=0, 
                       Length:=0, 
                       RunState:=_BUS_IF_RUN_STATE_NO_RUN,
                       bIsCtrlACC := FALSE) = FALSE then
          LogError("@076D (BusInterfaceVARAN::CyclicSDOHandling) Error occured at starting the MEM WriteDO");
        end_if;
        
        SDOHandling.pActEntry^.udState := 0;
        
        // after we successfully added this SDO, set the read pointer to the next element
        if SDOHandling.pActEntry = SDOHandling.pLastEntry then
          SDOHandling.pActEntry := SDOHandling.pRingBufMemStart;
        else
          SDOHandling.pActEntry += sizeof(t_SDORingBufEntry);
        end_if;
        
      end_if;
    end_if;
  end_if;
  
  OS_MT_SIGNAL(pv_MutexSDOs);
  
END_FUNCTION


FUNCTION VIRTUAL BusInterfaceVARAN::ChangeAsyRd
	VAR_INPUT
		Offset 	: HINT;
		Length 	: USINT;
		RunState 	: t_BusIFRunState;
		bIsCtrlACC 	: BOOL;
	END_VAR
	VAR_OUTPUT
		Success 	: BOOL;
	END_VAR
  VAR
    dataUPDATEDO    : t_UpdateDOCfg;
  	tempCmd         : CmdStruct;
    _result         : results;
    dataSETRUNSTATE : t_SetRunStateCfg;
  END_VAR

  // if length = zero we may just want to stop the DO
  if Length then
    
    if bIsCtrlACC then
      AsyCtrlReadDO.Offset := Offset;
      AsyCtrlReadDO.Length := Length;

      dataUPDATEDO.Handle               := AsyCtrlReadDO.Handle;
      dataUPDATEDO.ud_addressoff_read   := AsyCtrlReadDO.Offset;
      dataUPDATEDO.ud_length_read       := AsyCtrlReadDO.Length;
    
    else
      AsyMemReadDO.Offset := Offset;
      AsyMemReadDO.Length := Length;

      dataUPDATEDO.Handle               := AsyMemReadDO.Handle;
      dataUPDATEDO.ud_addressoff_read   := AsyMemReadDO.Offset;
      dataUPDATEDO.ud_length_read       := AsyMemReadDO.Length;
    end_if;
    
    tempCmd.uiCmd         := _BUS_IF_UPDATE_DO;  
    tempCmd.aPara[0]      := (#dataUPDATEDO)$DINT;  

    CallCommand.NewInst(pPara:=#tempCmd, pResult:=#_result); 
    if _result.aData[0]$t_BusIFRetvalErrorcode <> _BUS_IF_RETVAL_OK then  
      LogError("@007C (BusInterfaceVARAN::ChangeAsyRd) Failed to change asynchron read access");
    end_if;
  end_if;
  
  // set runstate
  if bIsCtrlACC then
    dataSETRUNSTATE.Handle      := AsyCtrlReadDO.Handle;
  else
    dataSETRUNSTATE.Handle      := AsyMemReadDO.Handle;
  end_if;

  dataSETRUNSTATE.NewRunState := RunState;
  
  tempCmd.uiCmd         := _BUS_IF_SET_RUN_STATE;  
  tempCmd.aPara[0]      := (#dataSETRUNSTATE)$DINT; 
 
  CallCommand.NewInst(pPara:=#tempCmd, pResult:=#_result); 
  
  if _result.aData[0]$t_BusIFRetvalErrorcode <> _BUS_IF_RETVAL_OK then
    LogError("@007D (BusInterfaceVARAN::ChangeAsyRd) Error changing the run state of the asynchron read access!");
  end_if; 
    
  Success := TRUE;

END_FUNCTION


