FUNCTION VIRTUAL BusInterfaceVARAN::SendPostInitFinished
	VAR_INPUT
		DisableThisObject 	: BOOL;(* := FALSE *)
	END_VAR
  VAR
  	tempCmd         : CmdStruct;
    _result         : results;
  END_VAR

  b_PostInitFinished := TRUE;
  tempCmd.uiCmd := _BUS_IF_POST_INIT_FINISHED;
  CallCommand.NewInst(pPara:=#tempCmd, pResult:=#_result);

  if _result.aData[0]$t_BusIFRetvalErrorcode <> _BUS_IF_RETVAL_OK then
    LogError("@00D8 (BusInterfaceVARAN::SendPostInitFinished) Failed to set the post init to finished via bus interface!");
  end_if;    


  if DisableThisObject then
    // tell the manager that we don't need to be called for rt syncing
    tempCmd.uiCmd := _BUS_IF_RT_SYNC_READY;
    CallCommand.NewInst(pPara:=#tempCmd, pResult:=#_result);
  
    if _result.aData[0]$t_BusIFRetvalErrorcode <> _BUS_IF_RETVAL_OK then
      LogError("@00D9 (BusInterfaceVARAN::CyWork) Failed to set rt sync ready via bus interface!");
    end_if;    
  end_if;
  

END_FUNCTION


FUNCTION VIRTUAL BusInterfaceVARAN::SimulateSDIASManagerCywork
VAR
	tempCmd           : CmdStruct; 
  _result           : results;    
  RetcodeIPR        : iprStates;
END_VAR  

  case eSdiasManagerInitSSW of
//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_StartInit:

      // remember when we started (for timeout and init time measure)
      InitStartTime := ops.tAbsolute;
    
      // check if we have a user class
      if p_UserCalls.p_Callback = NIL then
        // we already have a log entry from the constructor
        State := _CallBackError;
        LogError("@076E (BusInterfaceVARAN::SimulateSDIASManagerCywork) Pointer to user callback is invalid");        
        eErrorInitStep := eSdiasManagerInitSSW;
        eSdiasManagerInitSSW := _PrepareError;          
        return;
      end_if;
      
      //Go on with initialisation
      eSdiasManagerInitSSW := _InitAccesses;
      
//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_InitAccesses:
                 
      // call operator, who returns the init state (busy = call me again, error = error occured/cancel init, ready = init successfully finished)
      if p_UserCalls.p_Callback then
        // Call the corresponding function in the operator class
        tempCmd.uiCmd := _BUS_IF_INIT_ACCESSES;
        RetcodeIPR := p_UserCalls.p_Callback^.pSvrChCmd.NewInst(#tempCmd,#_result);
      else
        RetcodeIPR := READY;
      end_if;
      
      if RetcodeIPR = READY then
        eSdiasManagerInitSSW := t_SdiasManagerInitSSW::_FinishInitialisation;
      elsif RetcodeIPR = ERROR then
        eErrorInitStep := eSdiasManagerInitSSW;        
        eSdiasManagerInitSSW := t_SdiasManagerInitSSW::_PrepareError;
      end_if;
      
//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_FinishInitialisation:
    
      // tell the manager that we're finished with initializing, otherwise realtime will not be called
      if b_PostInitFinished = FALSE then
        SendPostInitFinished();
      end_if;
      
      //Enable the DOs in realtime
      eSdiasManagerInitSSW := _CallHwConnect;
      
      
//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_CallHwConnect:

      //Do nothing here, work is done in realtime (so that DOs will be started in the correct task
      
    
//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_ModuleInitialisation:
    
      // call operator, who returns the init state (busy = call me again, error = error occured/cancel init, ready = init successfully finished)
      if p_UserCalls.p_Callback then
        // Call the corresponding function in the operator class
        tempCmd.uiCmd := _BUS_IF_INIT_MODULE;
        RetcodeIPR := p_UserCalls.p_Callback^.pSvrChCmd.NewInst(#tempCmd,#_result);
      else
        RetcodeIPR := READY;
      end_if;
      
      if RetcodeIPR = READY then
        eSdiasManagerInitSSW := t_SdiasManagerInitSSW::_InitFinished;
        
        if bSendRtSyncReady = FALSE then
        
          bSendRtSyncReady := TRUE;
          //Finish init
          tempCmd.uiCmd := _BUS_IF_RT_SYNC_READY;
          CallCommand.NewInst(pPara:=#tempCmd, pResult:=#_result);
        
          if _result.aData[0]$t_BusIFRetvalErrorcode <> _BUS_IF_RETVAL_OK then
            LogError("@00D9 (BusInterfaceVARAN::CyWork) Failed to set rt sync ready via bus interface!");
          end_if;    
        end_if;        

      elsif RetcodeIPR = ERROR then
        eErrorInitStep := eSdiasManagerInitSSW;      
        eSdiasManagerInitSSW := t_SdiasManagerInitSSW::_PrepareError;
      end_if;
      
//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_InitFinished:

      p_UserCalls.p_UpdateCy^.CyWork(EAX:=0);

//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_PrepareError:

    // tell the manager that we're finished with initializing
    if b_PostInitFinished = FALSE then
      SendPostInitFinished(DisableThisObject:=TRUE);   
    end_if;
    
    //Sets a required error if the module is required
    SetRequiredError();

    eSdiasManagerInitSSW := _ErrorOccured;

//**********************************************************************************************************************************************************
    t_SdiasManagerInitSSW::_ErrorOccured:
      // do nothing here
      return;          
      
  end_case;
  
  //Always call SDO Handling
  CyclicSDOHandling();

END_FUNCTION


FUNCTION VIRTUAL BusInterfaceVARAN::SetRequiredError
  VAR
  	tempCmd         : CmdStruct;
    _result         : results;
  END_VAR

  if Required then
  
    // set required error
    tempCmd.uiCmd     := _BUS_IF_SET_REQUIRED_ERROR;
    
    CallCommand.NewInst(pPara:=#tempCmd, pResult:=#_result);
    if _result.aData[0]$t_BusIFRetvalErrorcode <> _BUS_IF_RETVAL_OK then
      LogError("@00A1 (BusInterfaceVARAN::SetRequiredError) Failed to set required error");
    end_if;    
  end_if;

END_FUNCTION


FUNCTION VIRTUAL BusInterfaceVARAN::SimulateSDIASManagerRtwork
VAR
	tempCmd     : CmdStruct;
  _result     : results;    
END_VAR
  
  case eSdiasManagerRTSSW of
  	t_SdiasManagerRtSSW::_RtIdle: // nothing
  	t_SdiasManagerRtSSW::_CreateAndModifyDOs: //nothing to do here
  	t_SdiasManagerRtSSW::_Wait4DOs:
  	t_SdiasManagerRtSSW::_Wait4PllSync:
  	t_SdiasManagerRtSSW::_Wait2Ticks:
  	t_SdiasManagerRtSSW::_CntDownSyncStart:
  	t_SdiasManagerRtSSW::_PreOperationalWait4DataReady:
  	t_SdiasManagerRtSSW::_PreOperational:
  	t_SdiasManagerRtSSW::_Operational:
  end_case;

  if eSdiasManagerInitSSW = _CallHwConnect then
  
    if bCallHwConnectInRT then
      bCallHwConnectInRT := FALSE;
      
      // Call the corresponding function in the operator class
      tempCmd.uiCmd := _BUS_IF_HWCONNECT;  
      p_UserCalls.p_Callback^.pSvrChCmd.NewInst(#tempCmd,#_result);
      
      eSdiasManagerInitSSW := _ModuleInitialisation;
      
      if _result.aData[0] then
        return;
      end_if;
    end_if;        
  end_if;

END_FUNCTION
