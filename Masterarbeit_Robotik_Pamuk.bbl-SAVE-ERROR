      }
      \strng{namehash}{607e58e5ef457d20627b0b87648c5cf3}
      \strng{fullhash}{607e58e5ef457d20627b0b87648c5cf3}
      \strng{fullhashraw}{607e58e5ef457d20627b0b87648c5cf3}
      \strng{bibnamehash}{607e58e5ef457d20627b0b87648c5cf3}
      \strng{authorbibnamehash}{607e58e5ef457d20627b0b87648c5cf3}
      \strng{authornamehash}{607e58e5ef457d20627b0b87648c5cf3}
