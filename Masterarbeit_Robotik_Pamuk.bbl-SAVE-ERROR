    \entry{canbazPerformanceAnalysisRealtime2022}{article}{}{}
      \name{author}{2}{}{%
        {{hash=878d7bd49bd08476702344f14acfb51e}{%
           family={Canbaz},
           familyi={C\bibinitperiod},
           given={Seçkin},
